pergunta = bloco.querySelector(\".pergunta\");\r\n            var resposta = bloco.querySelector(\".resposta\");\r\n            var p1 = bloco.querySelector(\".p1\");\r\n            var r1 = bloco.querySelector(\".r1\");\r\n\r\n            \/\/ Adiciona evento de clique para alternar visibilidade\r\n            pergunta.addEventListener(\"click\", function() {\r\n                if (resposta.style.display === \"none\" || resposta.style.display === \"\") {\r\n                    resposta.style.display = \"block\";\r\n                    p1.style.display = \"none\";\r\n                    r1.style.display = \"inline\";\r\n                } else {\r\n                    resposta.style.display = \"none\";\r\n                    p1.style.display = \"inline\";\r\n                    r1.style.display = \"none\";\r\n                }\r\n            });\r\n\r\n            \/\/ Esconde a resposta quando ela pr\u00f3pria for clicada\r\n            resposta.addEventListener(\"click\", function() {\r\n                resposta.style.display = \"none\";\r\n                p1.style.display = \"inline\";\r\n                r1.style.display = \"none\";\r\n            });\r\n        });\r\n    <\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-190c4891 elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"190c4891\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-72d0d138\" data-id=\"72d0d138\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<div class=\"element